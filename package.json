    "url": "git+https://github.com/panfiva/guacamole-common-js.git"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "type": "module",
  "devDependencies": {
    "axios": "^1.9.0",
    "gulp": "^5.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "tar": "^7.4.3",
    "xml2js": "^0.6.2"
  },
  "keywords": [
    "guacamole-common-js"
  ],
  "author": "Vadim Panfilov",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://issues.apache.org/jira/browse/GUACAMOLE/"
